# using utility pwgen
pwgen -h for options
pwgen 15 1
pwgen 15 1 -n # include one digit


# openssl
openssl rand -base64 14

# gpg
gpg --gen-random --armor 1 14

# using utility apg
apg
apg -a 1

# using utility xkcdpass
-> % xkcdpass    
pureness princess dipping placard rupture armadillo

# urandom
 < /dev/urandom tr -dc A-Za-z0-9 | head -c14; echo

