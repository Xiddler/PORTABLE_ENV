# stellarized_dark fish shell theme by haystackandroid (https://github.com/haystackandroid/stellarized)

# normal text
set fish_color_host              bead9d
set fish_color_normal            bead9d
set fish_pager_color_completion  bead9d
set fish_pager_color_description bead9d

# muted text
set fish_color_autosuggestion    9d8875
set fish_color_comment           9d8875

# reverse muted
set fish_pager_color_progress    222532 --background=#9d8875

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#4a5067
set fish_color_search_match      --background=#4a5067
set fish_color_selection         --background=#4a5067

# red errors/warnings
set fish_color_cancel            ca7375
set fish_color_cwd_root          ca7375
set fish_color_error             ca7375

# gold special elements
set fish_color_redirection       a58949
set fish_color_escape            a58949
set fish_color_operator          a58949
set fish_color_end               a58949

# green commands/status
set fish_color_command           5c9a61
set fish_color_cwd               5c9a61
set fish_color_user              5c9a61

# cyan parameters
set fish_color_param             009ba2

# blue quoted strings
set fish_color_quote             578fcc

# magenta pager name match
set fish_pager_color_prefix      b178b5

