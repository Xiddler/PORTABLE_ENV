(define-abbrev-table 'global-abbrev-table
  '(
    (" i " "I" nil :count 0 :case-fixed t)
    ("FOO" "facing our orriblenesses" nil :count 0 :case-fixed t)
    ("HH" "---~" nil :count 0 :case-fixed t)
    ("cant" "can't" nil :count 1)
    ("dont" "don't" nil :count 1)
    ("goo" "googling green glasses" nil :count 0 :case-fixed t)
    ("ii" "I" nil :count 0 :case-fixed t)
    ("wont" "won't" nil :count 1)
   ))

